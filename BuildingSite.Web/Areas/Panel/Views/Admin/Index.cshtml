            <div class="page-body">
                <div class="row">
                    <div class="col-sm-12">

                        <div class="card">
                            <div class="card-header">
                                <h5>Kullanıcı Listesi</h5>
                            </div>
                            <div>
                                <a href="/Panel/Admin/Create" class="btn btn-primary" style="float:right; margin-right:50px">Yeni Ekle</a>
                            </div>
                            <div class="card-block">
                                <div class="dt-responsive table-responsive">
                                    <table id="dom-table" class="table table-striped table-bordered nowrap">
                                        <thead>
                                            <tr>
                                                <th>Kullanıcı Adı</th>
                                                <th>Mail Adresi</th>
                                                <th>İşlemler</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var admin in Model)
                                            {
                                                <tr>
                                                    <td>@admin.UserName</td>
                                                    <td>@admin.Email</td>
                                                    <td>
                                                        <a href="/Panel/Admin/Edit/@admin.Id" class="btn btn-primary">Düzenle</a>&nbsp;&nbsp;&nbsp;<a href="#" class="btn btn-danger">Sil</a>
                                                    </td>
                                                </tr>
                                            }
                                        </tbody>
                                        <tfoot>
                                            <tr>
                                                <th>Kullanıcı Adı</th>
                                                <th>Mail Adresi</th>
                                                <th>İşlemler</th>
                                            </tr>
                                        </tfoot>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>


